{
  "name": "khalti/php-sdk",
  "description": "PHP SDK for Khalti Payment Gateway",
  "type": "library",
  "license": "MIT",
  "authors": [
      {
          "name": "Khalti SDK Team",
          "email": "support@khalti.com"
      }
  ],
  "require": {
      "php": ">=7.2",
      "ext-curl": "*",
      "ext-json": "*"
  },
  "require-dev": {
      "phpunit/phpunit": "^8.0",
      "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
      "psr-4": {
          "KhaltiSDK\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "KhaltiSDK\\Tests\\": "tests/"
      }
  },
  "scripts": {
      "test": "phpunit",
      "cs": "phpcs",
      "cs-fix": "phpcbf"
  },
  "config": {
      "sort-packages": true,
      "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}